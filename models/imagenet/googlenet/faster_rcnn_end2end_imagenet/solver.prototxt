train_net: "models/imagenet/googlenet/faster_rcnn_end2end_imagenet/train.prototxt"
base_lr: 0.0001
lr_policy: "step"
gamma: 0.1
stepsize: 30000
display: 20
average_loss: 100
# iter_size: 1
momentum: 0.9
weight_decay: 0.0005
# We disable standard caffe solver snapshotting and implement our own snapshot
# function
snapshot: 0
# We still use the snapshot prefix, though
snapshot_prefix: "googlenet_faster_rcnn"
iter_size: 2
